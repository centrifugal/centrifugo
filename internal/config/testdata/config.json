{
  "tls": {
    "enabled": true
  },
  "client": {
    "allowed_origins": ["http://localhost:3000"],
    "token": {
      "jwks_public_endpoint": "https://example.com/jwks"
    },
    "ping_interval": "12s",
    "proxy": {
      "connect": {
        "http": {
          "static_headers": {
            "x": "y"
          }
        }
      }
    }
  },
  "channel": {
    "without_namespace": {
        "presence": true
    },
    "namespaces": [
      {
        "name": "ns1",
        "presence": true
      },
      {
        "name": "ns2",
        "presence": false
      }
    ]
  },
  "proxies": [
    {
      "name": "ppp"
    }
  ],
  "consumers": [
    {
      "name": "kafka",
      "enabled": true,
      "type": "kafka",
      "kafka": {
        "brokers": ["localhost:9092"],
        "topics": ["postgres.public.chat_cdc"],
        "consumer_group": "centrifugo",
        "tls": {
          "enabled": true,
          "cert_pem": "xxx"
        }
      }
    }
  ],
  "websocket": {
    "write_timeout": "2s"
  }
}
